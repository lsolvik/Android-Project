<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndreLeggTil"
    tools:layout_editor_absoluteY="25dp">

    <Button
        android:id="@+id/leggTilTwist"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="235dp"
        android:text="@string/legg_til_twist"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/leggTilBilde"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginTop="235dp"
        android:layout_marginEnd="16dp"
        android:text="@string/legg_til_bilde"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/leggTilLek"
        android:layout_width="175dp"
        android:layout_height="70dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="104dp"
        android:text="@string/leggTilLek"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/leggTilTwist" />
</android.support.constraint.ConstraintLayout>